{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [ "main" ],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "dialog:default",
    "http:default",
    "fs:default",
    "shell:allow-open",
    {
      "identifier": "fs:scope",
      "allow": ["$APPDATA/**"]
    }
  ]
}
